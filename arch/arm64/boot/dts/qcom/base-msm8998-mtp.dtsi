&ufs_ice {
	status = "ok";
};

&sdhc_2 {
	vdd-supply = <&pm8998_l21>;
	qcom,vdd-voltage-level = <2950000 2960000>;
	qcom,vdd-current-level = <200 800000>;

	vdd-io-supply = <&pm8998_l13>;
	qcom,vdd-io-voltage-level = <1808000 2960000>;
	qcom,vdd-io-current-level = <200 22000>;

	pinctrl-names = "active", "sleep";
	pinctrl-0 = <&sdc2_clk_on  &sdc2_cmd_on &sdc2_data_on &sdc2_cd_on>;
	pinctrl-1 = <&sdc2_clk_off &sdc2_cmd_off &sdc2_data_off &sdc2_cd_off>;

	qcom,clk-rates = <400000 20000000 25000000
				50000000 100000000 200000000>;
	qcom,bus-speed-mode = "SDR12", "SDR25", "SDR50", "DDR50", "SDR104";

	cd-gpios = <&tlmm 95 0x1>;

	status = "ok";
};

&uartblsp2dm1 {
	status = "ok";
	pinctrl-names = "default";
	pinctrl-0 = <&uart_console_active>;
};

&pm8998_gpios {
	/* GPIO 2 for Home Key */
	key_home {
		key_home_default: key_home_default {
			pins = "gpio2";
			function = "normal";
			input-enable;
			bias-pull-up;
		};
	};

	/* GPIO 6 for Vol+ Key */
	key_vol_up {
		key_vol_up_default: key_vol_up_default {
			pins = "gpio6";
			function = "normal";
			input-enable;
			bias-pull-up;
		};
	};

	/* GPIO 7 for Snapshot Key */
	key_cam_snapshot {
		key_cam_snapshot_default: key_cam_snapshot_default {
			pins = "gpio7";
			function = "normal";
			input-enable;
			bias-pull-up;
		};
	};

	/* GPIO 8 for Focus Key */
	key_cam_focus {
		key_cam_focus_default: key_cam_focus_default {
			pins = "gpio8";
			function = "normal";
			input-enable;
			bias-pull-up;
		};
	};

	pm8998_gpio13 { /* GPIO 13 */
		pm8998_gpio13_default: pm8998_gpio13_default {
			pins = "gpio13";
			function = "func2";
			output-low;
			drive-push-pull;
			bias-disable;
		};
	};

	/* GPIO 21 (NFC_CLK_REQ) */
	nfc_clk_req {
		nfc_clk_req_default: nfc_clk_req_default {
			pins = "gpio21";
			function = "normal";
			input-enable;
			power-source = <1>;
		};
	};

	/* GPIO 18 SMB138X */
	smb138x_gpio {
		smb138x_gpio_default: smb138x_gpio_default {
			pins = "gpio18";
			function = "normal";
			input-enable;
			bias-pull-up;
		};
	};
};

